{
  "defaultSeverity": "warning",
  "extends": "tslint-config-airbnb",
  "rules": {
    "prefer-array-literal": false,
    "max-line-length": [true, 180],
    "variable-name": false,
    "ter-arrow-parens": false,
    "ter-indent": false,
    "radix": false,
    "no-parameter-reassignment": false,
    "comment-format": false
  }
}